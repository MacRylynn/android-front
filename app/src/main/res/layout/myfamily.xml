            android:text="Joseph"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="300dp"
            android:text=" James"
            android:textSize="20sp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="340dp"
            android:text="Henry"
            android:textSize="20sp" />


    </RelativeLayout>
