<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#ffffff"
    android:orientation="vertical">

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/relativeLayout"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="#FFF8F8F8"
        android:orientation="vertical">

        <ImageView
            android:layout_width="18sp"
            android:layout_height="18sp"
            android:layout_marginLeft="3sp"
            android:layout_marginTop="33dp"
            android:src="@mipmap/pointleft" />

        <TextView
            android:id="@+id/textView1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_gravity="center_horizontal"
            android:layout_marginTop="0dp"
            android:singleLine="false"
            android:text="摩米视觉实验室"
            android:textAlignment="center"
            android:textColor="#000"
            android:textSize="10sp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20sp"
            android:layout_marginTop="30dp"
            android:clickable="true"
            android:focusable="true"
            android:singleLine="false"
            android:text="返回"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textColor="@color/colorAccent"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="60dp"
            android:singleLine="false"
            android:text="开启检查"
            android:textAlignment="center"
            android:textColor="#000"
            android:textSize="35sp" />

        <ImageView
            android:layout_width="30sp"
            android:layout_height="30sp"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:layout_marginEnd="25dp"
            android:src="@drawable/head" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="450sp"
        android:layout_marginTop="110sp">

        <TableLayout
            android:layout_width="340sp"
            android:layout_height="450sp"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true">

            <TableRow android:layout_marginTop="5sp">

                <ImageButton android:src="@mipmap/result" />

                <ImageButton android:src="@mipmap/result" />

                <ImageButton android:src="@mipmap/result" />
            </TableRow>

            <TableRow android:layout_marginTop="5sp">

                <ImageButton android:src="@mipmap/result" />

                <ImageButton android:src="@mipmap/result" />

                <ImageButton android:src="@mipmap/result" />
            </TableRow>

            <TableRow android:layout_marginTop="5sp">

                <ImageButton android:src="@mipmap/result" />

                <ImageButton android:src="@mipmap/result" />

                <ImageButton android:src="@mipmap/result" />
            </TableRow>

            <TableRow android:layout_marginTop="5sp">

                <ImageButton android:src="@mipmap/result" />

                <ImageButton android:src="@mipmap/result" />

                <ImageButton android:src="@mipmap/result" />
            </TableRow>
        </TableLayout>
    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="450sp"
        android:layout_marginTop="110sp">

        <TableLayout
            android:layout_width="340sp"
            android:layout_height="450sp"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true">

            <TableRow android:layout_marginTop="5sp">
                <TextView android:text="AUG"
                    android:id="@+id/result1"
                    android:layout_marginTop="35sp"
                    android:layout_marginStart="40sp"/>
                <TextView android:text="HJK"
                    android:id="@+id/result2"
                    android:layout_marginTop="35sp"
                    android:layout_marginStart="80sp"/>
                <TextView android:text="EFG"
                    android:id="@+id/result3"
                    android:layout_marginTop="35sp"
                    android:layout_marginStart="80sp"/>
            </TableRow>
            <TableRow android:layout_marginTop="0sp">
                <TextView android:text="1"
                    android:id="@+id/result1-v"
                    android:textAlignment="center"
                    android:layout_marginTop="0sp"
                    android:layout_marginStart="40sp"/>
                <TextView android:text="1"
                    android:id="@+id/result2-v"
                    android:textAlignment="center"
                    android:layout_marginTop="0sp"
                    android:layout_marginStart="80sp"/>
                <TextView android:text="1"
                    android:textAlignment="center"
                    android:id="@+id/result3-v"
                    android:layout_marginTop="0sp"
                    android:layout_marginStart="80sp"/>
            </TableRow>
            <TableRow android:layout_marginTop="35sp">
                <TextView android:text="AUG"
                    android:id="@+id/result4"
                    android:layout_marginTop="35sp"
                    android:layout_marginStart="40sp"/>
                <TextView android:text="EDG"
                    android:id="@+id/result5"
                    android:layout_marginTop="35sp"
                    android:layout_marginStart="80sp"/>
                <TextView android:text="CBD"
                    android:id="@+id/result6"
                    android:layout_marginTop="35sp"
                    android:layout_marginStart="80sp"/>
            </TableRow>
            <TableRow android:layout_marginTop="0sp">
                <TextView android:text="1"
                    android:id="@+id/result4-v"
                    android:textAlignment="center"
                    android:layout_marginTop="0sp"
                    android:layout_marginStart="40sp"/>
                <TextView android:text="1"
                    android:id="@+id/result5-v"
                    android:textAlignment="center"
                    android:layout_marginTop="0sp"
                    android:layout_marginStart="80sp"/>
                <TextView android:text="1"
                    android:textAlignment="center"
                    android:id="@+id/result6-v"
                    android:layout_marginTop="0sp"
                    android:layout_marginStart="80sp"/>
            </TableRow>
            <TableRow android:layout_marginTop="35sp">
                <TextView android:text="AUG"
                    android:id="@+id/result7"
                    android:layout_marginTop="35sp"
                    android:layout_marginStart="40sp"/>
                <TextView android:text="LGD"
                    android:id="@+id/result8"
                    android:layout_marginTop="35sp"
                    android:layout_marginStart="80sp"/>
                <TextView android:text="RND"
                    android:id="@+id/result9"
                    android:layout_marginTop="35sp"
                    android:layout_marginStart="80sp"/>
            </TableRow>
            <TableRow android:layout_marginTop="0sp">
                <TextView android:text="1"
                    android:id="@+id/result7-v"
                    android:textAlignment="center"
                    android:layout_marginTop="0sp"
                    android:layout_marginStart="40sp"/>
                <TextView android:text="1"
                    android:id="@+id/result8-v"
                    android:textAlignment="center"
                    android:layout_marginTop="0sp"
                    android:layout_marginStart="80sp"/>
                <TextView android:text="1"
                    android:textAlignment="center"
                    android:id="@+id/result9-v"
                    android:layout_marginTop="0sp"
                    android:layout_marginStart="80sp"/>
            </TableRow>
            <TableRow android:layout_marginTop="35sp">
                <TextView android:text="RLD"
                    android:id="@+id/result10"
                    android:layout_marginTop="35sp"
                    android:layout_marginStart="40sp"/>
                <TextView android:text="BLD"
                    android:id="@+id/result11"
                    android:layout_marginTop="35sp"
                    android:layout_marginStart="80sp"/>
                <TextView android:text="  PH"
                    android:id="@+id/result12"
                    android:layout_marginTop="35sp"
                    android:layout_marginStart="80sp"/>
            </TableRow>
            <TableRow android:layout_marginTop="0sp">
                <TextView android:text="1"
                    android:id="@+id/result10-v"
                    android:textAlignment="center"
                    android:layout_marginTop="0sp"
                    android:layout_marginStart="40sp"/>
                <TextView android:text="1"
                    android:id="@+id/result11-v"
                    android:textAlignment="center"
                    android:layout_marginTop="0sp"
                    android:layout_marginStart="80sp"/>
                <TextView android:text="1"
                    android:textAlignment="center"
                    android:id="@+id/result12-v"
                    android:layout_marginTop="0sp"
                    android:layout_marginStart="80sp"/>
            </TableRow>

        </TableLayout>
    </RelativeLayout>
    <Button
        android:id="@+id/button8"
        android:layout_width="380sp"
        android:layout_height="wrap_content"
        android:layout_above="@+id/textView9"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="24dp"
        android:text="显示检测结果"
        android:textColor="#FF505050" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="match_parent"
        android:layout_height="30sp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_gravity="center_horizontal"
        android:singleLine="false"
        android:text=" @copyright 2019 MMLB"
        android:textAlignment="center"
        android:textSize="15sp" />


</RelativeLayout>
